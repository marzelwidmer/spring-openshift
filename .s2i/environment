MVN_ARGS=--settings=configuration/settings.xml -Dhttps.protocols=TLSv1.2 -Dmaven.wagon.http.ssl.insecure=true -Dmaven.wagon.http.ssl.allowall=true -Dcom.sun.jndi.ldap.object.disableEndpointIdentification
insecure-skip-tls-verify=true
insecure-registry=true
